    <!--GPRS/CDMA-->  
    <GPRS addr="" localport="8088"/> 
  </communication>  
  <!--数据库-->  
  <database ip="localhost" port="3306" dbname="gil"/>  
  <start jflib="" gzlib=""/> 
</property>
